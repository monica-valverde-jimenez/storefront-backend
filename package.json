{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --config .prettierrc '*.js' --write",
    "prettier-js": "prettier --config .prettierrc 'build/**/*.js' --write",
    "prettier-ts": "prettier --config .prettierrc src/**/*.ts --write",
    "test": "npm run build && ENV_NODE=test db-migrate --env test up && jasmine-ts && db-migrate --env test reset",
    "test-windows": "npm run build && set ENV_NODE=test && db-migrate --env test reset && db-migrate --env test up && jasmine-ts && db-migrate --env test reset",
    "tsc": "tsc"
  },
  "author": "Monika Valverde Jimenez",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^14.2.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "prettier": "^2.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.6.3",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
